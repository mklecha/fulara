<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		 version="3.1">
	<context-param>
		<param-name>jdbcString</param-name>
		<param-value>jdbc:mysql://127.0.0.1:3306/fulara?useUnicode=true&amp;characterEncoding=utf8</param-value>
	</context-param>
	<context-param>
		<param-name>jdbcUser</param-name>
		<param-value>fulara</param-value>
	</context-param>
	<context-param>
		<param-name>jdbcPassword</param-name>
		<param-value>fulara123</param-value>
	</context-param>
	<context-param>
		<param-name>ftlTemplateDir</param-name>
		<param-value>/home/fulary/templates/</param-value>
	</context-param>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/static/*</url-pattern>
	</servlet-mapping>
</web-app>
